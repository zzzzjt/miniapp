/* components/table/table.wxss */
.table {
  border: 0px solid darkgray;
  font-size: small;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    justify-content: center;
    text-align: center;
}
.th {

  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}